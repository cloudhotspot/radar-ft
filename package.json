{
  "name": "radar-ft",
  "version": "0.0.1",
  "keywords": [],
  "description": "radar functional tests",
  "contributors": [],
  "jshintConfig": {
    "node": true,
    "undef": true,
    "noarg": true,
    "white": true,
    "esnext": true,
    "strict": true,
    "newcap": true,
    "nonbsp": true,
    "eqeqeq": true,
    "unused": true,
    "maxlen": 80,
    "indent": 2,
    "bitwise": true,
    "noempty": true,
    "latedef": "nofunc",
    "maxdepth": 5,
    "quotmark": "single",
    "trailing": true,
    "camelcase": true,
    "maxparams": 8,
    "globalstrict": true,
    "maxcomplexity": 10,
    "maxstatements": 40,
    "predef": [
      "ApiError",
      "ActErr",
      "logger",
      "util",
      "sendErrorAndLog",
      "shipError",
      "sendJSONResponse",
      "config",
      "MessageTypes",
      "safeGet"
    ]
  }
}
